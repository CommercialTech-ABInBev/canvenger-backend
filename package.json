        "@azure/msal-node": "^1.1.0",
        "@azure/storage-blob": "^12.6.0",
        "@babel/polyfill": "^7.12.1",
        "@hapi/joi": "^15.1.1",
        "@types/hapi__joi": "^15.0.3",
        "azure-storage": "^2.10.4",
        "bcryptjs": "^2.4.3",
        "chai": "^4.2.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.5",
        "errorhandler": "^1.5.1",
        "exceljs": "^4.2.0",
        "express": "^4.17.1",
        "express-jwt": "^6.0.0",
        "express-session": "^1.17.1",
        "joi-password-complexity": "^3.1.0",
        "jsonwebtoken": "^8.5.1",
        "method-override": "^3.0.0",
