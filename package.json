{
    "name": "scanvenger",
    "version": "1.0.0",
    "description": "This is an app for ib travels",
    "main": "index.js",
    "scripts": {
        "lint": "eslint",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=html mocha src/test/*.js --timeout 10000 -c --require @babel/register --exit",
        "start": "node dist/index.js",
        "build": "babel src -d dist",
        "migrate": "sequelize db:migrate ",
        "migrate:reset": "sequelize db:migrate:undo:all",
        "seed": "sequelize db:seed:all",
        "migrate:seed": "npm run migrate && npm run seed",
        "start:dev": "npm run migrate && nodemon --exec babel-node ./src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CommercialTech-ABInBev/Ib_Travels.git"
    },
    "keywords": [
        "auth",
        "general",
        "abi",
        "login"
    ],
    "author": "Tochukwu Ozurumba",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/CommercialTech-ABInBev/Ib_Travels/issues"
    },
    "homepage": "https://github.com/CommercialTech-ABInBev/Ib_Travels#readme",
